 version = "7.0"
@@ -118,3 +119,3 @@
 
-[dev-dependencies.hdrhistogram]
+[disabled.dev-dependencies.hdrhistogram]
 version = "7.0"
@@ -188,3 +189,3 @@
     "filter",
-    "hedge",
