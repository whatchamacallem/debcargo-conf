Description: Temporarily disable B-D on log to unblock build
 This crate and its B-D on log is in a chain that bidirectionally blocks regex
 and regex-automata. No-op +logging to try to unblock it.
Last-Update: 2023-10-02
---
This patch header follows DEP-3: http://dep.debian.net/deps/dep3/
--- a/Cargo.toml
+++ b/Cargo.toml
@@ -54,5 +54,5 @@
 
-[dependencies.log]
-version = "0.4.17"
-optional = true
+#[dependencies.log]
+#version = "0.4.17"
+#optional = true
 
@@ -71,3 +71,3 @@
 ]
-logging = ["dep:log"]
+#logging = ["dep:log"]
 perf-literal = ["dep:memchr"]
--- a/src/macros.rs
+++ b/src/macros.rs
@@ -12,3 +12,3 @@
 macro_rules! debug {
-    ($($tt:tt)*) => { log!(log::debug!($($tt)*)) }
+    ($($tt:tt)*) => { }// log!(log::debug!($($tt)*)) }
 }
@@ -16,3 +16,3 @@
 macro_rules! trace {
-    ($($tt:tt)*) => { log!(log::trace!($($tt)*)) }
+    ($($tt:tt)*) => { }// log!(log::trace!($($tt)*)) }
 }
