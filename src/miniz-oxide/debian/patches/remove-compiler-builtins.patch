Description: remove compiler-builtins feature.
 The feature fails to build and the ftpmasters are rejecting new rust
 feature packages.
Author: Peter Michael Green <plugwash@debian.org>

--- rust-miniz-oxide-0.4.4.orig/Cargo.toml
+++ rust-miniz-oxide-0.4.4/Cargo.toml
@@ -32,10 +32,6 @@ name = "miniz_oxide"
 version = "1.1.0"
 default-features = false
 
-[dependencies.compiler_builtins]
-version = "0.1.2"
-optional = true
-
 [build-dependencies.autocfg]
 version = "1.0"
 
